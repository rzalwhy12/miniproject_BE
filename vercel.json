{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm run build",
                "installCommand": "npm install",
                "nodeVersion": "20.x",
                "includeFiles": ["dist/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}